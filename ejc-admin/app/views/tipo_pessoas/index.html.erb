<p id="notice"><%= notice %></p>

<h1>Tipo Pessoas</h1>

<table>
  <thead>
    <tr>
      <th>Des tipo pessoa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_pessoas.each do |tipo_pessoa| %>
      <tr>
        <td><%= tipo_pessoa.des_tipo_pessoa %></td>
        <td><%= link_to 'Show', tipo_pessoa %></td>
        <td><%= link_to 'Edit', edit_tipo_pessoa_path(tipo_pessoa) %></td>
        <td><%= link_to 'Destroy', tipo_pessoa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Pessoa', new_tipo_pessoa_path %>
