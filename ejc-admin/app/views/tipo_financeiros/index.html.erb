<p id="notice"><%= notice %></p>

<h1>Tipo Financeiros</h1>

<table>
  <thead>
    <tr>
      <th>Des tipo financeiro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_financeiros.each do |tipo_financeiro| %>
      <tr>
        <td><%= tipo_financeiro.des_tipo_financeiro %></td>
        <td><%= link_to 'Show', tipo_financeiro %></td>
        <td><%= link_to 'Edit', edit_tipo_financeiro_path(tipo_financeiro) %></td>
        <td><%= link_to 'Destroy', tipo_financeiro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Financeiro', new_tipo_financeiro_path %>
